{
  "manifest_version": 2,
  "name": "CopyIt",
  "version": "2.0",
  "description": "Enables copy in Terv platform",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "enable_paste.js"
      ]
    }
  ]
}